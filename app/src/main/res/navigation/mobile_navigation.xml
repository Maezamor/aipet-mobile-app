<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.capstone.aipet.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_listDogsFragment_to_detailDogsFragment"
            app:destination="@id/navigation_detaildog" />
    </fragment>
    <fragment
        android:id="@+id/navigation_detaildog"
        android:name="com.capstone.aipet.ui.home.detaildog.DetailDogFragment"
        android:label="detaildog"
        tools:layout="@layout/fragment_detail_dog"/>
    <fragment
        android:id="@+id/navigation_history"
        android:name="com.capstone.aipet.ui.history.HistoryFragment"
        android:label="@string/history"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/navigation_maps"
        android:name="com.capstone.aipet.ui.maps.MapsFragment"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.capstone.aipet.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile"/>
</navigation>